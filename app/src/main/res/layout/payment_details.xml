<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:background="@color/colorWhite"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/container_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include

            android:id="@+id/toolbar"
            layout="@layout/toolbar" />
    </LinearLayout>

    <ScrollView
        android:scrollbars="none"
        android:fillViewport="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <LinearLayout
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingBottom="10dp"
            android:background="@color/colorWhite"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:layout_gravity="center_horizontal"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:id="@+id/imgcompany"
                android:layout_marginTop="20dp"
                android:visibility="gone"
                />
            <LinearLayout
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical"

                android:layout_marginTop="5dp"
                >
                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    >android:layout_weight="1"
                    <TextView
                        android:fontFamily="serif"
                        android:paddingLeft="10dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/nav_PaymentDetails"
                        android:textStyle="bold"
                        android:gravity="left"
                        android:layout_gravity="left"
                        android:textColor="#000000"
                        android:textSize="15sp"
                        />

                    <Spinner

                        android:layout_margin="10dp"
                        android:id="@+id/spinner_period"
                        android:layout_marginTop="5dp"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textSize="12sp"
                        style="@style/Widget.AppCompat.Spinner.Underlined"
                        />
                    android:layout_marginLeft="5dp"
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout

                            android:layout_weight="1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:padding="5dp">

                            <TextView
                                android:fontFamily="serif"
                                android:layout_gravity="center_vertical|center_horizontal"
                                android:gravity="center"
                                android:layout_weight="1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/from"
                                android:textSize="15sp"
                                android:textColor="@android:color/black"
                                android:textStyle="bold"
                                />

                            <EditText
                                android:fontFamily="serif"
                                android:textSize="12sp"
                                android:textColor="@drawable/disable"
                                android:layout_weight="1"
                                android:layout_gravity="center_horizontal"
                                android:gravity="center"
                                android:focusable="false"
                                android:cursorVisible="false"
                                android:drawableRight="@android:drawable/ic_menu_my_calendar"
                                android:id="@+id/from"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:onClick="onDate"

                                android:text="YYYY-MM-DD"/>
                            android:onClick="onDate"
                        </LinearLayout>

                        <LinearLayout

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:padding="5dp">

                            <TextView
                                android:textSize="15sp"
                                android:fontFamily="serif"
                                android:layout_gravity="center_vertical|center_horizontal"
                                android:gravity="center"
                                android:layout_weight="1"
                                android:textStyle="bold"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/To"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColor="@android:color/black"
                                />

                            <EditText
                                android:fontFamily="serif"
                                android:textSize="12sp"
                                android:layout_gravity="center_horizontal"
                                android:textColor="@drawable/disable"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:focusable="false"
                                android:cursorVisible="false"
                                android:drawableRight="@android:drawable/ic_menu_my_calendar"
                                android:id="@+id/to"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:onClick="onDate"
                                android:text="YYYY-MM-DD"/>
                            android:onClick="onDate"
                        </LinearLayout>

                    </LinearLayout>


                    <Button
                        android:fontFamily="serif"
                        android:layout_marginTop="10dp"
                        android:textColor="@color/colorWhite"
                        android:background="@color/colorGreen"
                        android:id="@+id/btnGetDetails"
                        android:layout_gravity="center_horizontal"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginBottom="10dp"
                        android:text="@string/getData"/>

                    aasdasdasd
                    android:visibility="gone"
                    <LinearLayout
                        android:layout_marginTop="10dp"
                        android:background="@color/colorDarkYellow"
                        android:id="@+id/linearDetails"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">




                        <LinearLayout
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="5dp"
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:visibility="gone"
                                android:padding="5dp"
                                android:weightSum="5"
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:fontFamily="serif"
                                    android:gravity="left"
                                    android:layout_weight="2.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="Online Payment"
                                    android:textColor="@color/colorBlack"

                                    />

                                <TextView
                                    android:fontFamily="serif"
                                    android:id="@+id/txtOnlineEarning"
                                    android:gravity="right"
                                    android:layout_weight="1.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="₹ 0"
                                    android:textColor="@color/colorBlack"
                                    />


                            </LinearLayout>

                            <LinearLayout
                                android:padding="5dp"
                                android:weightSum="5"
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:fontFamily="serif"
                                    android:gravity="left"
                                    android:layout_weight="2.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="@string/opening_balance"
                                    android:textColor="@color/colorBlack"

                                    />

                                <TextView
                                    android:fontFamily="serif"
                                    android:id="@+id/txtOpeningBal"
                                    android:gravity="right"
                                    android:layout_weight="1.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="₹ 0"
                                    android:textColor="@color/colorBlack"
                                    />


                            </LinearLayout>

                            <LinearLayout
                                android:padding="5dp"
                                android:weightSum="5"
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:fontFamily="serif"
                                    android:gravity="left"
                                    android:layout_weight="2.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="@string/totalearning"
                                    android:textColor="@color/colorBlack"

                                    />

                                <TextView
                                    android:fontFamily="serif"
                                    android:id="@+id/txtTotalEarning"
                                    android:gravity="right"
                                    android:layout_weight="1.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="₹ 0"
                                    android:textColor="@color/colorBlack"
                                    />


                            </LinearLayout>


                            <LinearLayout
                                android:padding="5dp"
                                android:weightSum="5"
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:fontFamily="serif"
                                    android:gravity="left"
                                    android:layout_weight="2.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="@string/cashcollect"
                                    android:textColor="@color/colorBlack"

                                    />

                                <TextView
                                    android:fontFamily="serif"
                                    android:id="@+id/txtCashCollect"
                                    android:gravity="right"
                                    android:layout_weight="1.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="₹ 0"
                                    android:textColor="@color/colorBlack"
                                    />


                            </LinearLayout>


                            <LinearLayout
                                android:padding="5dp"
                                android:weightSum="5"
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:fontFamily="serif"
                                    android:gravity="left"
                                    android:layout_weight="2.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="@string/fees"
                                    android:textColor="@color/colorBlack"

                                    />

                                <TextView
                                    android:fontFamily="serif"
                                    android:id="@+id/txtFees"
                                    android:gravity="right"
                                    android:layout_weight="1.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="₹ 0"
                                    android:textColor="@color/colorBlack"
                                    />


                            </LinearLayout>


                            <LinearLayout
                                android:padding="5dp"
                                android:weightSum="5"
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:fontFamily="serif"
                                    android:gravity="left"
                                    android:layout_weight="2.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="@string/totalpayout"
                                    android:textColor="@color/colorBlack"

                                    />

                                <TextView
                                    android:fontFamily="serif"
                                    android:id="@+id/txtTotalPayout"
                                    android:gravity="right"
                                    android:layout_weight="1.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="₹ 0"
                                    android:textColor="@color/colorBlack"
                                    />


                            </LinearLayout>

                            <LinearLayout
                                android:padding="5dp"
                                android:weightSum="5"
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:fontFamily="serif"
                                    android:gravity="left"
                                    android:layout_weight="2.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="@string/payment"
                                    android:textColor="@color/colorBlack"

                                    />

                                <TextView
                                    android:fontFamily="serif"
                                    android:id="@+id/txtPayment"
                                    android:gravity="right"
                                    android:layout_weight="1.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="₹ 0"
                                    android:textColor="@color/colorBlack"
                                    />


                            </LinearLayout>

                            <LinearLayout
                                android:padding="5dp"
                                android:weightSum="5"
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:fontFamily="serif"
                                    android:gravity="left"
                                    android:layout_weight="2.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="@string/totalbalance"
                                    android:textColor="@color/colorBlack"

                                    />

                                <TextView
                                    android:fontFamily="serif"
                                    android:id="@+id/txtTotalBal"
                                    android:gravity="right"
                                    android:layout_weight="1.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="₹ 0"
                                    android:textColor="@color/colorBlack"
                                    />


                            </LinearLayout>


                            <LinearLayout
                                android:padding="5dp"
                                android:weightSum="5"
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:fontFamily="serif"
                                    android:gravity="left"
                                    android:layout_weight="2.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="@string/dayendclosing"
                                    android:textColor="@color/colorBlack"

                                    />

                                <TextView
                                    android:fontFamily="serif"
                                    android:id="@+id/txtDayEndClosing"
                                    android:gravity="right"
                                    android:layout_weight="1.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="₹ 0"
                                    android:textColor="@color/colorBlack"
                                    />


                            </LinearLayout>


                            <LinearLayout
                                android:padding="5dp"
                                android:weightSum="5"
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:fontFamily="serif"
                                    android:gravity="left"
                                    android:layout_weight="2.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="@string/amtpaidtocomp"
                                    android:textColor="@color/colorBlack"

                                    />

                                <TextView
                                    android:fontFamily="serif"
                                    android:id="@+id/txtamtPaidToComp"
                                    android:gravity="right"
                                    android:layout_weight="1.5"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="₹ 0"
                                    android:textColor="@color/colorBlack"
                                    />


                            </LinearLayout>


                        </LinearLayout>



                    </LinearLayout><!--fsdf-->





                </LinearLayout>




            </LinearLayout>
        </LinearLayout>

    </ScrollView>

</LinearLayout>


